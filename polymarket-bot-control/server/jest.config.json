{
  "testEnvironment": "node",
  "testMatch": [
    "**/__tests__/**/*.test.js",
    "**/?(*.)+(spec|test).js"
  ],
  "collectCoverageFrom": [
    "server.js",
    "!node_modules/**"
  ],
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "setupFilesAfterEnv": [],
  "testTimeout": 30000
}
